{namespace soy.examples.let}
/**
* Template that uses object and array literal values.
* @param currentYear
*/
{template .demoObjectLiterals}
{let $emptyArray: []/}
{let $emptyObject: [:]/}
{let $arrayOfEmptyObject: [[:],[:]]/}
{call .demoExpressions}
  {param currentYear: $currentYear/}
  {param students: [
  ['name':'Bill','major':'Engineering','year':1988],
  ['name':'Matt','major':'Physics','year':2002],
  ['name':'John','major':'Biology','year':1996],
  ['name':'Arthur','major':'English','year':2006]
]/}
{/call}
{/template}
